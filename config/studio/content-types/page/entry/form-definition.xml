               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>path</name>
                     <value/>
